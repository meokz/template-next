{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "format": "eslint src --ext .js,jsx,.ts,.tsx --fix ; prettier --write --ignore-path .prettierignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "lint": "next lint --dir src && yarn build",
    "ci": "next lint --dir src && prettier --ignore-path .prettierignore './**/*.{js,jsx,ts,tsx,json,css}' && yarn build"
  },
  "dependencies": {
    "next": "13.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.3"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^1.0.2",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "eslint": "8.28.0",
    "eslint-config-next": "13.0.4",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1"
  }
}
